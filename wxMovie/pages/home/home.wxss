/* pages/home/home.wxss */
@import '/pages/movieGrid/movieGrid';
.search-placeholder{
  color: #d9d9d9;
}
.session-page{
  background: #fff;
  border-width: 1rpx;
  border-style: solid;
  border-color: #e9e9e9;
  padding-bottom: 30rpx;
  margin-bottom: 30rpx;
}
.session-header{
  display: flex;
  padding: 20rpx 30rpx;
  align-items: center;

}
.head-title{
  color: #4a4a4a;
  flex: 1;
  font-weight: 500;
  font-size: 36rpx;

}
.head-more{
  color:#32dd32
}
.session-content{
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
  white-space: nowrap;
}
.movie-scroll{
  white-space: nowrap;
  width: 100%;

}
